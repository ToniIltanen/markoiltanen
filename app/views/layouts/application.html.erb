<!DOCTYPE html>
<html>
  <head>
    <title>Marko Iltanen Eduskuntaan!</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <!--Kode Wrapper Start-->
  <div class="kode_wrapper">
    <%= render '/layouts/header' %>

    <%= yield %>





    <%= render '/layouts/footer' %>
  </div>

    <%= javascript_include_tag 'jquery' %>
    <%= javascript_include_tag 'bootstrap' %>
    <%= javascript_include_tag 'jquery.bxslider.min' %>
    <%= javascript_include_tag 'jquery.downCount' %>
    <%= javascript_include_tag 'owl.carousel.min' %>
    <%= javascript_include_tag 'waypoints-min' %>
    <%= javascript_include_tag 'jquery.accordion' %>
    <%= javascript_include_tag 'TimeCircles' %>
    <%= javascript_include_tag 'jquery.prettyPhoto' %>
    <%= javascript_include_tag 'jquery-filterable' %>
    <%= javascript_include_tag 'modernizr.custom' %>
    <%= javascript_include_tag 'jquery.dlmenu' %>
    <script src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <%= javascript_include_tag 'moment.min' %>
    <%= javascript_include_tag 'fullcalendar.min' %>
    <%= javascript_include_tag 'custom' %>


  </body>
</html>
